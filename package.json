{
  "name": "mealkit",
  "author": "Ella Nan",
  "description": "A simple meal planner app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "vercel dev",
    "dev": "vite --port $PORT",
    "build": "vite build",
    "serve": "vite preview",
    "db:reset": "npx prisma db push && npm run db:seed",
    "db:seed": "npx ts-node prisma/seed.ts",
    "dev:server": "npx prisma generate --watch",
    "client-codegen": "graphql-codegen --config codegen.yml",
    "postinstall": "prisma generate",
    "rebuild-schema": "ts-node --transpile-only api/_helpers/makeSchema.ts",
    "check-heroku-connections": "heroku pg:psql -c 'select usename, application_name, backend_start, query, wait_event, backend_type from pg_stat_activity;' --app meal-kit-app | grep dlvlnllqztaxbz | wc -l",
    "kill-heroku-connections": "heroku pg:killall",
    "electron": "electron .",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "./node_modules/.bin/electron-builder -c.extraMetadata.main=build/electron.js -mw",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "inigo-darwin-arm64": "^0.30.1",
    "inigo-linux-amd64": "^0.30.1",
    "inigo.js": "^0.30.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/client": "3.9.9",
    "@apollo/server": "4.10.2",
    "@as-integrations/next": "^3.0.0",
    "@auth0/auth0-react": "^1.8.0",
    "@chakra-ui/icons": "^1.0.16",
    "@chakra-ui/react": "^1.6.10",
    "@ellanan/graphql-iso-date": "3.6.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-document-nodes": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@prisma/client": "^3.5.0",
    "@sentry/node": "^6.18.2",
    "@sentry/react": "^6.18.2",
    "@sentry/tracing": "^6.18.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tinymce/tinymce-react": "^3.14.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.175",
    "@types/luxon": "^2.0.5",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^20.0.0",
    "@types/react": "^17.0.0",
    "@types/react-date-range": "^1.4.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@types/stylis": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vercel/node": "^3.0.24",
    "@vitejs/plugin-react": "^4.2.1",
    "allotment": "^1.20.0",
    "apollo-server-cloud-functions": "3.13.0",
    "autoprefixer": "^9.8.8",
    "classnames": "^2.3.1",
    "concurrently": "^6.5.1",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "framer-motion": "^4.1.17",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "next": "^12.3.4",
    "nexus": "^1.1.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prisma": "^3.5.0",
    "react": "^17.0.2",
    "react-date-range": "^1.4.0",
    "react-dom": "^17.0.2",
    "react-icons": "4.3.1",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.1.0",
    "stylis": "^4.3.6",
    "tailwindcss": "3.4.1",
    "ts-node": "^10.2.1",
    "typescript": "4.4.3",
    "typewriter-effect": "^2.18.2",
    "usehooks-ts": "^3.0.2",
    "vercel": "^33.6.1",
    "vite": "^5.2.2",
    "vite-plugin-svgr": "4.2.0",
    "vite-plugin-vercel": "^4.0.2",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1",
    "winston": "^3.3.3"
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@10.19.0",
  "volta": {
    "node": "22.21.0",
    "npm": "10.9.4"
  },
  "build": {
    "appId": "com.ellanan.mealkit",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "portable"
      ]
    }
  }
}
